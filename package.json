{
  "name": "forta-dashboard",
  "author": "Artem Kovalchuk",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config config/webpack.config.dev.js --open",
    "build": "cross-env NODE_ENV=development webpack --config config/webpack.config.prod.js",
    "lint:js": "eslint src --ext .js --ext .jsx",
    "lint:js:fix": "eslint src --ext .js --ext .jsx --fix",
    "lint:style": "stylelint \"./src/**/*.scss\"",
    "lint:fix": "npm run lint:js:fix && npm run format",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\"",
    "dep-list": "npm ls --depth=0 --prod=true --long=true",
    "prepare": "husky install",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run lint:js:fix",
      "npm run format"
    ],
    "*.{scss,css}": [
      "npm run lint:style",
      "npm run format"
    ]
  },
  "browserslist": {
    "production": [
      "last 2 version",
      "> 1%",
      "IE 11"
    ],
    "development": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version",
      "last 2 ie version"
    ]
  },
  "dependencies": {
    "@ahooksjs/use-url-state": "^3.5.0",
    "@floating-ui/react-dom-interactions": "^0.9.2",
    "@popperjs/core": "^2.11.5",
    "@react-hook/window-size": "^3.0.7",
    "@types/yup": "^0.32.0",
    "async": "^3.2.4",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "color-interpolate": "^1.0.5",
    "copy-to-clipboard": "^3.3.2",
    "dayjs": "^1.11.3",
    "dexie": "^3.2.2",
    "ethers": "^5.6.9",
    "focus-visible": "^5.2.0",
    "formik": "^2.2.9",
    "lodash": "^4.17.21",
    "lru-cache": "^7.13.2",
    "normalize.css": "^8.0.1",
    "overlayscrollbars": "^1.13.2",
    "overlayscrollbars-react": "^0.3.0",
    "pluralize": "^8.0.0",
    "prop-types": "^15.8.1",
    "query-string": "^7.1.1",
    "range-interpolator": "^1.0.2",
    "react": "^18.2.0",
    "react-collapse": "^5.1.1",
    "react-confirm": "^0.2.3",
    "react-cool-onclickoutside": "^1.7.0",
    "react-dom": "^18.2.0",
    "react-flatpickr": "^3.10.12",
    "react-imask": "^6.4.2",
    "react-infinite-scroll-hook": "^4.0.3",
    "react-modal": "^3.15.1",
    "react-query": "^3.39.2",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-select": "^5.3.2",
    "react-textarea-autosize": "^8.3.4",
    "react-tippy": "^1.4.0",
    "react-toastify": "^9.0.5",
    "react-transition-group": "^4.4.2",
    "use-debounce": "^8.0.3",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.5",
    "@babel/plugin-transform-runtime": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@types/async": "^3.2.15",
    "autoprefixer": "^10.4.7",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "cssnano": "^5.1.12",
    "dotenv-webpack": "^7.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-custom-alias": "^1.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss-loader": "^7.0.0",
    "prettier": "^2.7.1",
    "sass": "^1.52.3",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.3.1",
    "stylelint": "^13.8.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.18.0",
    "svg-sprite-loader": "^6.0.11",
    "svgo-loader": "^3.0.1",
    "terser-webpack-plugin": "^5.3.3",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.0"
  }
}
